<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ifi.kuirin.baseanroid">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.recyclerview.RecyclerActivity"/>
        <activity android:name=".ui.baseview.BaseViewSelectedActivity"/>
        <activity android:name=".ui.recyclerview.DragAndDropRecyclerViewActivity"/>
        <activity android:name=".ui.dialog_fragment.DialogFragmentActivity"/>
        <activity android:name=".ui.coordinatorlayout.CoordinatorActivity"/>
    </application>

</manifest>